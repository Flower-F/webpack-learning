{
  "name": "my first extension",
  "version": "1.0.0",
  "description": "This is my first extension",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "matches": [
        "https://*.mi.com/*"
      ]
    }
  ],
  "icons": {
    "16": "cool.png",
    "48": "cool.png",
    "128": "cool.png"
  },
  "browser_action": {
    "default_title": "Created by Flower-F. Enjoy it!"
  }
}